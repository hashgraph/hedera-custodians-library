    "@types/node": "^18.11.18",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-license-header": "^0.6.0",
    "husky": "^9.0.10",
    "inquirer": "^9.2.14",
