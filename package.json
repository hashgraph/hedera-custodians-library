    "node-fetch": "^3.3.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tweetnacl": "^1.0.3",
    "typescript": "^5.2.0"
  },
  "dependencies": {
    "@dfns/sdk": "~0.3.1",
    "@dfns/sdk-keysigner": "~0.3.1",
    "@fireblocks/fireblocks-web3-provider": "^1.2.5",
    "dotenv": "^16.4.3",
    "fireblocks-sdk": "^5.9.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
